APP_NAME=fltg
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8000

APP_LOCALE=ru
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=ru_RU

LOG_CHANNEL=stack
LOG_LEVEL=debug

# PostgreSQL. For Docker use DATABASE_URL (set by compose).
# Local: DB_* below or DATABASE_URL="postgresql://growth:growth@127.0.0.1:5432/growth"
DB_CONNECTION=pgsql
DB_HOST=growth_pg
DB_PORT=5432
DB_DATABASE=growth
DB_USERNAME=growth
DB_PASSWORD=growth

SESSION_DRIVER=file
SESSION_LIFETIME=120

CACHE_STORE=file
QUEUE_CONNECTION=sync
